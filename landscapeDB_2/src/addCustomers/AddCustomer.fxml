<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane hgap="10.0" vgap="5.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="addCustomers.AddCustomerController">
   <columnConstraints>
      <ColumnConstraints maxWidth="92.0" minWidth="36.0" prefWidth="75.0" />
      <ColumnConstraints maxWidth="141.0" minWidth="85.0" prefWidth="102.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0" />
      <RowConstraints maxHeight="25.0" minHeight="25.0" prefHeight="25.0" />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label text="First Name" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
      <Label text="Last Name" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
      <Button fx:id="add" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#addCustomerButton" text="Add" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="4" />
      <TextField fx:id="last" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <TextField fx:id="first" GridPane.columnIndex="1" GridPane.rowIndex="1" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="15.0" top="5.0" />
   </padding>
</GridPane>
