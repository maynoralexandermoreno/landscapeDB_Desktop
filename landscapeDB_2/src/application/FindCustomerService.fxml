<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane hgap="10.0" prefHeight="147.0" prefWidth="202.0" vgap="5.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FindCustomerServiceController">
   <columnConstraints>
      <ColumnConstraints maxWidth="177.0" minWidth="0.0" prefWidth="85.0" />
      <ColumnConstraints maxWidth="177.0" minWidth="0.0" prefWidth="92.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="25.0" minHeight="25.0" prefHeight="25.0" />
      <RowConstraints />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0" />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label text="Customer Id" GridPane.halignment="CENTER" />
      <Label text="Item Id" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
      <Button fx:id="add" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#addServiceButton" text="Add" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
      <Label text="Customer Id:" GridPane.halignment="CENTER" />
      <Label text="Price" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
      <Button fx:id="searchID" mnemonicParsing="false" onAction="#findCustomerId" text="Search" GridPane.columnIndex="1" GridPane.halignment="RIGHT" />
      <Button mnemonicParsing="false" onAction="#findItemId" text="Search" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
      <Label fx:id="result" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</GridPane>
